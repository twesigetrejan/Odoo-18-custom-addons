<odoo>
    <data noupdate="1">
        <record id="investments_account_sequence_number" model="ir.sequence">
            <field name="name">Investments Account Sequence</field>
            <field name="code">sacco.investments.account</field>
            <field name="prefix">INV/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
        </record>

        <record id="investments_pool_sequence_number" model="ir.sequence">
            <field name="name">Investments Pool Sequence</field>
            <field name="code">sacco.investments.pool</field>
            <field name="prefix">INV/POOL/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
        </record>

        <record id="investments_transaction_sequence_number" model="ir.sequence">
            <field name="name">Investments Transaction Sequence</field>
            <field name="code">sacco.investments.transaction</field>
            <field name="prefix">TRANS/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="investments_withdrawal_request_sequence_number" model="ir.sequence">
            <field name="name">Withdrawal Request Sequence</field>
            <field name="code">sacco.investments.withdrawal.request</field>
            <field name="prefix">WITH/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_investment_product_code" model="ir.sequence">
            <field name="name">Investment Product Code</field>
            <field name="code">sacco.investment.product.code</field>
            <field name="padding">3</field>
            <field name="prefix">IP</field>
        </record>

        <record id="sequence_investment_pool_profit" model="ir.sequence">
            <field name="name">Investment Pool Profit Sequence</field>
            <field name="code">sacco.investment.pool.profit</field>
            <field name="prefix">IPP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>