<odoo>
    <data noupdate="1">
        <record id="savings_account_sequence_number" model="ir.sequence">
            <field name="name">Savings Account Sequence</field>
            <field name="code">sacco.savings.account</field>
            <field name="prefix">SAV/</field>
            <field name="padding">3</field>
            <field name="use_date_range">True</field>
        </record>

        <record id="savings_transaction_sequence_number" model="ir.sequence">
            <field name="name">Savings Transaction Sequence</field>
            <field name="code">savings.transaction</field>
            <field name="prefix">TRANS/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="withdrawal_request_sequence_number" model="ir.sequence">
            <field name="name">Withdrawal Request Sequence</field>
            <field name="code">sacco.withdrawal.request</field>
            <field name="prefix">WITH/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="sequence_interest_posting" model="ir.sequence">
            <field name="name">Interest Posting Sequence</field>
            <field name="code">sacco.interest.posting</field>
            <field name="prefix">INT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_savings_product_code" model="ir.sequence">
            <field name="name">Savings Product Code</field>
            <field name="code">sacco.savings.product.code</field>
            <field name="padding">3</field>
            <field name="prefix">SP</field>
        </record>
    </data>
</odoo>